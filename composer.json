{
    "name": "LarsWiegers/Test",
    "description": "TEST_PACKAGE_DESCRIPTION",
    "keywords": [
        "test",
        "test"
    ],
    "homepage": "https://github.com/LarsWiegers/Test",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "TEST_AUTHOR_NAME",
            "email": "TEST_AUTHOR_EMAIL",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Test\\Test\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Test\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Test\\Test\\TestServiceProvider"
            ],
            "aliases": {
                "Test": "Test\\Test\\TestFacade"
            }
        }
    }
}
